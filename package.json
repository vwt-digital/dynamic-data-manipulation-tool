    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "~11.2.4",
    "protractor": "~5.4.3",
    "semantic-release": "^17.1.1",
    "tslint": "~6.1.0",
    "typescript": "~3.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vwt-digital/dynamic-data-manipulation-tool.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira"
