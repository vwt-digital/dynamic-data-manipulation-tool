    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "codelyzer": "^5.1.2",
    "commitizen": "^4.1.2",
    "eslint": "^7.2.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
